CODE - 
multiples_of_ten = 12
A = []
multiples_of_eight = 120
B = []
multiples_of_five = 9
C = []
multiples_of_eight2 = 300 
D = []

while multiples_of_ten >= 12 and multiples_of_ten < 100:
    if multiples_of_ten % 10 == 0:
        A.append(multiples_of_ten)
    multiples_of_ten = multiples_of_ten + 1
print(A)

while multiples_of_eight <= 120 and multiples_of_eight >= 20:
    if multiples_of_eight % 8 == 0:
        B.append(multiples_of_eight)
    multiples_of_eight = multiples_of_eight - 1
print(B)    

while multiples_of_five >= 9 and multiples_of_five <= 40:
    if multiples_of_five % 5 == 0:
        C.append(multiples_of_five)
    multiples_of_five = multiples_of_five + 1
print(C)

